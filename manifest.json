{
    "manifest_version": 3,
    "name": "tab saver",
    "icons": { "128": "icon.png" },
    "description": "Save and open Chrome tabs.",
    "version": "1.0.0",
    "permissions": ["tabs", "scripting", "activeTab", "downloads"],
    "action": {
        "default_popup": "./index.html",
        "default_icon": "icon.png"
    }
}
